{
  "name": "step-into-story-time",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type:check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "validate": "npm run lint && npm run type:check && npm test",
    "secrets:generate": "powershell -ExecutionPolicy Bypass -File ./scripts/setup-secrets.ps1",
    "secrets:scan": "npx trufflehog filesystem .",
    "pre-commit": "npx trufflehog filesystem .",
    "test:ui": "vitest --ui",
    "build:clean": "rimraf .next",
    "format": "prettier --write .",
    "fix:types": "node scripts/type-migration.js",
    "storybook": "",
    "build-storybook": "",
    "analyze": "node scripts/analyze-bundle.js",
    "monitor": "node scripts/performance-monitor.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject .next && sentry-cli sourcemaps upload .next",
    "build:sentry": "npm run disable:sentry && next build --no-lint && npm run restore:sentry",
    "build:with-sentry": "next build && npm run sentry:sourcemaps",
    "sentry:test": "sentry-cli test",
    "verify:deployment": "node scripts/verify-deployment.js",
    "verify:env": "node scripts/verify-env.js",
    "test:sentry": "node scripts/test-sentry.js",
    "deploy:prepare": "npm run build:clean && npm run build:sentry && npm run verify:deployment",
    "deploy:verify-only": "npm run verify:deployment",
    "deploy:vercel": "vercel --prod",
    "security:check": "node scripts/security-check.js",
    "deploy": "node scripts/deploy.js",
    "setup:vercel": "node scripts/setup-vercel-env.js",
    "setup:sentry": "node scripts/setup-sentry.js",
    "disable:sentry": "node scripts/disable-sentry.js",
    "restore:sentry": "node scripts/restore-sentry.js",
    "setup:local": "node scripts/setup-local-env.js",
    "fix:verify": "node scripts/fix-verify-env.js",
    "fix:deployment": "node scripts/fix-deployment.js",
    "generate:env-script": "node scripts/generate-env-script.js",
    "postinstall": "if [ \"$VERCEL\" != \"1\" ]; then husky install; fi || true",
    "prepare": "if [ \"$VERCEL\" != \"1\" ]; then husky install; fi || true"
  },
  "dependencies": {
    "@agentdeskai/browser-tools-mcp": "^1.0.11",
    "@clerk/nextjs": "^6.12.1",
    "@emotion/is-prop-valid": "^1.3.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-jaeger": "^1.22.0",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.49.1",
    "@opentelemetry/semantic-conventions": "^1.30.0",
    "@sentry/nextjs": "^9.2.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.49.1",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/kv": "^0.2.4",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.7.9",
    "chalk": "^5.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "critters": "^0.0.23",
    "dotenv": "^16.0.3",
    "framer-motion": "^11.18.2",
    "is-arguments": "^1.2.0",
    "is-generator-function": "^1.1.0",
    "is-typed-array": "^1.1.15",
    "lucide-react": "^0.477.0",
    "next": "^15.1.7",
    "openai": "^4.29.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "svix": "^1.61.0",
    "swr": "^2.3.2",
    "tailwind-merge": "^2.6.0",
    "use-sound": "^5.0.0",
    "uuid": "^11.0.5",
    "web-vitals": "^4.2.4",
    "which-typed-array": "^1.1.18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.0.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.8",
    "@types/react": "19.0.10",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.0.7",
    "autoprefixer": "^10.4.20",
    "compression-webpack-plugin": "^11.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.1.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.2.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.2",
    "url-loader": "^4.1.1",
    "vitest": "^3.0.7",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  },
  "engines": {
    "node": "22.x",
    "npm": ">=10.0.0"
  }
}
